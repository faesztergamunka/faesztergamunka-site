/ Wrapper
#wrapper
  / Header
  %header#header
    .inner
      / Logo
      %a.logo{:href => "index.html"}
        %span.symbol
          %img{:alt => "", :src => "images/logo.svg"}/
        %span.title= data.site.logotitle
      / Nav
      %nav
        %ul
          %li
            %a{:href => "#menu"} Menu
  / Menu
